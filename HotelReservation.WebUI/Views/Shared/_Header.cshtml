@{
    Layout = null;
}

<div class="container">
    <div class="row">
        <div class="col-xl-7 col-lg-7 col-md-6 d-none d-xl-block d-sm-block">
            <div class="top-header-content">
                <ul class="list-none">
                    @await Component.InvokeAsync("HeaderContact")
                </ul>
            </div>
        </div>
        <div class="col-xl-5 col-lg-5 col-md-6 col-sm-6 col-8 d-none d-block d-sm-block">
            <div class="top-header-content">
                <div class="top-social">
                    @await Component.InvokeAsync("HeaderSocialMedia")                   
                </div>
            </div>
        </div>
    </div>
</div>